          "_sourceTheme": "carm4",
          "_customHTML": "<section class=\"tabs02 tabs carm4_tabs2\" data-bg-video=\"{{bg.type == 'video'&&bg.value.url}}\" group=\"Tabs\" mbr-class=\"{'mbr-parallax-background':bg.parallax, 'mbr-fullscreen': fullScreen}\" plugins=\"mbr-tabs\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input name=\"fullWidth\" title=\"Full Width\" type=\"checkbox\">\n        <input name=\"fullScreen\" title=\"Full Screen\" type=\"checkbox\">\n        <input condition=\"fullScreen == false\" inline max=\"9\" min=\"0\" name=\"paddingTop\" step=\"1\" title=\"Top\" type=\"range\" value=\"4\">\n        <input condition=\"fullScreen == false\" inline max=\"9\" min=\"0\" name=\"paddingBottom\" step=\"1\" title=\"Bottom\" type=\"range\" value=\"4\">\n        <header>Show/Hide</header>\n        <input checked name=\"showImages\" title=\"Images\" type=\"checkbox\">\n        <input checked name=\"showTitle\" title=\"Title\" type=\"checkbox\">\n        <input checked name=\"showSubtitle\" title=\"Subtitle\" type=\"checkbox\">\n        <input checked name=\"showText\" title=\"Text\" type=\"checkbox\">\n        <select name=\"bootstrapTabs\" title=\"Tabs Count\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n        </select>\n        <header>Colors</header>\n        <input name=\"tabsColor\" title=\"Text\" type=\"color\" value=\"#06182d\">\n        <input name=\"tabBgColor\" title=\"BG\" type=\"color\" value=\"#f4f6f9\">\n        <input name=\"activeTabColor\" title=\"Active Tab Bg\" type=\"color\" value=\"#ff513c\">\n        <input name=\"activeTabTextColor\" title=\"Active Tab Text\" type=\"color\" value=\"#ffffff\">\n        <header condition=\"showTitle\">Mask</header>\n        <input condition=\"showTitle\" name=\"showTitleMask\" title=\"Title\" type=\"checkbox\" checked>\n        <input condition=\"showTitleMask && showTitle\" name=\"maskTextColor\" selected title=\"Text\" type=\"color\" value=\"#ffffff\">\n        <input condition=\"showTitleMask && showTitle\" name=\"gradientMask\" title=\"Gradient Mask\" type=\"checkbox\" checked>\n        <input condition=\"showTitleMask && showTitle\" name=\"maskColor\" title=\"Color 1\" type=\"color\" value=\"#ff5e3a\">\n        <input condition=\"gradientMask && showTitleMask && showTitle\" name=\"maskColor2\" title=\"Color 2\" type=\"color\" value=\"#ff653c\">\n        <input condition=\"gradientMask && showTitleMask && showTitle\" name=\"maskColor3\" title=\"Color 3\" type=\"color\" value=\"#ff4e3c\">\n        <hr>\n        <input name=\"reverse\" title=\"Content reverse\" type=\"checkbox\">\n        <fieldset name=\"bg\" parallax type=\"background\">\n            <input parallax title=\"Image\" type=\"image\" value=\"../_images/background3.jpg\">\n            <input selected title=\"Color\" type=\"color\" value=\"#ffffff\">\n            <input title=\"Video\" type=\"video\" value=\"http://www.youtube.com/watch?v=uNCr7NdOJgw\">\n        </fieldset>\n        <input checked condition=\"bg.type !== 'color'\" name=\"overlay\" title=\"Overlay\" type=\"checkbox\">\n        <input condition=\"overlay&&bg.type !== 'color'\" name=\"overlayColor\" title=\"Overlay Color\" type=\"color\" value=\"#ffffff\">\n        <input condition=\"overlay&&bg.type !== 'color'\" inline max=\"1\" min=\"0\" name=\"overlayOpacity\" step=\"0.1\" title=\"Opacity\" type=\"range\" value=\"0.7\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n    <div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n        <div class=\"row mt-4\">\n            <div class=\"col-12 col-md-12 col-lg-4\">\n\n                <ul class=\"nav nav-tabs\" role=\"tabl